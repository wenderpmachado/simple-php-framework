{
    "name": "wenderpmachado/simple-php-framework",
    "description": "Simple php framework using some libraries interesting",
    "type": "framework",
    "require": {
        "slim/slim": "^3.8",
        "vlucas/phpdotenv": "^2.4",
        "phputil/json": "^1.2",
        "phputil/pdowrapper": "^1.0",
        "phputil/di": "^0.3.0",
        "phputil/rtti": "^1.3",
        "phputil/traits": "^1.0",
        "robmorgan/phinx": "^0.6.5",
        "symfony/console": "^3.2",
        "slim/php-view": "^2.2",
        "monolog/monolog": "^1.22",
        "myclabs/php-enum": "^1.5"
    },
    "require-dev": {
        "composer/composer": "dev-master"
    },
    "autoload": {
        "classmap" : [
            "src/"
        ],
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/"
        }
    }
}
